programa {
  funcao inicio() {
   
   real celsius,kelvin,fahrenheint

    escreva("Temperatura: ")
    leia(celsius)

    fahrenheint = celsius * 1.8 +32
    kelvin = celsius + 273.15

    escreva("Temperatura em Kelvin: " +kelvin)
    escreva("\n Temperatura em Fahrenheit: " +fahrenheint)


  }
}
