programa {
  funcao inicio() {
   real positivo,negativo,zero,numero

     escreva("Escolha um número: ")
     leia(numero)

     se (numero > 0)
     escreva("\n esse numero é positivo:")

     se(numero < 0)
     escreva("\n esse número é negativo")

     se(numero == 0 )
     escreva("\n esse numero é igual a zero")



  }
}
